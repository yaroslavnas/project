	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="LoaderScreens.UserControl1"
	x:Name="UserControl"
	d:DesignWidth="75" d:DesignHeight="49.463">

	<Grid x:Name="LayoutRoot">
		<Button Content="Button" Style="{DynamicResource Button-Sketch}" BorderBrush="{x:Null}" Foreground="{x:Null}">
